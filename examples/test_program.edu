programa TestProgram;

var x: inteiro;
var y: inteiro;
var arr: array [10] de inteiro;

inicio
    x = 5;
    y = 10;
    
    escrever("Valor de x: ", x);
    escrever("Valor de y: ", y);
    
    se x < y entao
        escrever("x é menor que y");
    senao
        escrever("x é maior ou igual a y");
    fimse;
    
    para i de 0 ate 9 faca
        arr[i] = i * 2;
    fimpara;
    
    enquanto x < 20 faca
        x = x + 1;
    fimenquanto;
    
    escrever("Valor final de x: ", x);
fimprograma
